{
  "name": "switchboard-epl-feeds",
  "version": "1.0.0",
  "description": "A small example script on producing EPL data feeds and publishing to Switchboard's oracle network.",
  "author": "jack@switchboard.xyz",
  "license": "MIT",
  "private": true,
  "scripts": {
    "publish": "",
    "build": "tsc",
    "dev": "ts-node src/main.ts --payerKeypairFile=example-keypair.json --fulfillmentManager=${FULFILLMENT_MANAGER_KEY?}",
    "start": "tsc && node bin/main.js --payerKeypairFile=example-keypair.json --fulfillmentManager=${FULFILLMENT_MANAGER_KEY?}",
    "create-ff-manager": "ts-node src/utils/createFFManager.ts --payerKeypairFile=example-keypair.json",
    "lint": "eslint --ext .js,.json,.ts 'src/**' --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessupjn/switchboard-epl-feeds.git"
  },
  "keywords": [
    "switchboard"
  ],
  "bugs": {
    "url": "https://github.com/jessupjn/switchboard-epl-feeds/issues"
  },
  "homepage": "https://github.com/jessupjn/switchboard-epl-feeds#readme",
  "dependencies": {
    "@solana/web3.js": "^1.29.2",
    "@switchboard-xyz/switchboard-api": "^0.2.152",
    "@types/yargs": "^17.0.3",
    "chalk": "^4.1.2",
    "fs": "^0.0.1-security",
    "readline-sync": "^1.4.10",
    "resolve-dir": "^1.0.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
